{
	"publisher": "baincd",
	"name": "extension-profiles",
	"displayName": "Extension Profiles",
	"description": "Define profiles of extension, and quickly enable different profiles in different workspaces",
	"version": "0.0.1",
	"license": "MIT",
	"engines": {
		"vscode": "^1.32.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/baincd/vscode-extension-profiles"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onStartupFinished"
	],
	"main": "./out/main.js",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint . --ext .ts,.tsx",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/chai": "^4.2.15",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^12.12.0",
		"@types/vscode": "^1.32.0",
		"@typescript-eslint/eslint-plugin": "^4.16.0",
		"@typescript-eslint/parser": "^4.16.0",
		"chai": "^4.3.4",
		"eslint": "^7.21.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"source-map-support": "^0.5.12",
		"typescript": "^4.2.2",
		"vscode-test": "^1.3.0"
	}
